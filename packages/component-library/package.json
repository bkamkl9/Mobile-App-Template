{
  "name": "component-library",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/component-library.cjs",
  "module": "./dist/component-library.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/component-library.js",
      "require": "./dist/component-library.cjs"
    },
    "./index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "build:web": "vite build && npx vue-tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "vite build --watch & npx vue-tsc --emitDeclarationOnly --declaration --outDir dist --watch"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/vite": "^4.1.14",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript-config": "workspace:*",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0"
  }
}
